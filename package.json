{
  "name": "unice-backend",
  "version": "1.0.0",
  "description": "unice app backend",
  "author": "GenesisLab",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" && eslint --fix --ext .ts .",
    "start": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register src/main.ts",
    "start:test": "cross-env NODE_ENV=test nodemon",
    "start:dev": "cross-env nodemon",
    "start:debug": "cross-env NODE_ENV=development nodemon",
    "prestart:prod": "rm -rf dist && tsc && tsc-alias",
    "start:prod": "node worker.js",
    "start:hmr": "node dist/server",
    "clear_jest": "jest --clearCache",
    "test": "cross-env NODE_ENV=test  jest --config ./test/jest-e2e.json --detectOpenHandles --forceExit",
    "test:cov": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --coverage --detectOpenHandles --forceExit",
    "test:watch": "cross-env NODE_ENV=test jest --watch --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js",
    "ts-typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "migrate:fresh": "yarn ts-typeorm schema:drop -d src/database/data-source.ts && yarn ts-typeorm migration:run -d src/database/data-source.ts && yarn seed:run -d src/database/data-source.ts"
  },
  "dependencies": {
    "@langchain/community": "^0.3.39",
    "@langchain/core": "^0.3.43",
    "@langchain/groq": "^0.2.1",
    "@langchain/openai": "^0.5.4",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.2.10",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.10",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.2.10",
    "@nestjs/platform-socket.io": "^10.4.15",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/websockets": "^10.2.10",
    "@stream-io/node-sdk": "^0.4.19",
    "@stream-io/video-client": "^1.18.7",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "apple-signin-auth": "^2.0.0",
    "authenticator": "^1.1.5",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^9.6.3",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.20",
    "moment": "^2.29.4",
    "moralis": "^2.27.2",
    "nest-winston": "^1.9.4",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "typescript": "^5.3.2",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "winston-console-transport": "^1.1.9"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.2.10",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.99",
    "@swc/jest": "^0.2.29",
    "@types/bcrypt": "^5.0.2",
    "@types/bull": "^3.15.9",
    "@types/compression": "^1.7.5",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
